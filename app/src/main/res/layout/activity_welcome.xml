<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f2ebd0"
    tools:context=".welcome">


    <ImageButton
        android:id="@+id/comingSoon"
        android:layout_width="wrap_content"
        android:layout_height="211dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="202dp"
        android:layout_marginTop="398dp"
        android:layout_marginEnd="13dp"
        android:background="@null"
        android:scaleType="centerInside"
        app:srcCompat="@drawable/ic_comingsoonlogo"
        />

    <ImageButton
        android:id="@+id/manualTracking"
        android:layout_width="191dp"
        android:layout_height="192dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="10dp"
        android:layout_marginTop="400dp"
        android:background="@null"
        android:scaleType="centerInside"
        app:srcCompat="@drawable/ic_manualtrackinglogo" />

    <ImageButton
        android:id="@+id/poker"
        android:layout_width="241dp"
        android:layout_height="334dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="177dp"
        android:layout_marginTop="123dp"
        android:layout_marginEnd="-7dp"
        android:background="@null"
        android:scaleType="centerInside"
        app:srcCompat="@drawable/ic_pokerlogo" />

    <ImageView
        android:id="@+id/logoscoretrack"
        android:layout_width="wrap_content"
        android:layout_height="233dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="5dp"
        android:layout_marginTop="-36dp"
        android:layout_marginEnd="17dp"
        android:src="@drawable/ic_logoscoretrack2"></ImageView>

    <ImageButton
        android:id="@+id/blackjack"
        android:layout_width="259dp"
        android:layout_height="230dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="-25dp"
        android:layout_marginTop="177dp"
        android:background="@null"
        android:scaleType="centerInside"
        app:srcCompat="@drawable/ic_blackjacklogo" />


</RelativeLayout>